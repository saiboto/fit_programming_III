my$bottom_diam_y <- my$bottom_diam_y + 1
my$top_diam_x <- my$top_diam_x - 1
my$top_diam_y <- my$top_diam_y - 1
new_filename <- paste0("My200-", diam_sd, "-2-0.csv")
